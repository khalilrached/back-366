  "main": "app.js",
  "type": "module",
  "scripts": {
    "build": "babel server.js --out-file server.js && babel src --out-dir build",
    "lint": "eslint source/ --quiet",
    "start": "npm run build && node server.js",
    "start:dev": "nodemon --exec babel-node server.js",
    "start:debug": "node --inspect server.js",
    "test": "for i in $(ls tests/); do babel-node \"./tests/${i}\" | faucet ; done",
    "validate": "npm run lint; npm run test && npm outdated --depth 0"
  },
  "dependencies": {
    "@hapi/joi-date": "^2.0.1",
    "axios": "^0.25.0",
    "body-parser": "^1.20.0",
    "compression": "^1.7.4",
    "connect-flash": "^0.1.1",
    "cors": "^2.8.5",
    "dotenv": "^15.0.0",
    "express": "^4.17.2",
    "express-boom": "^3.0.0",
    "express-session": "^1.17.2",
    "express-validation": "^3.0.8",
    "fs": "0.0.1-security",
    "helmet": "^5.0.2",
    "joi": "^17.6.0",
    "mime": "^3.0.0",
    "module-alias": "^2.2.2",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.4-lts.1",
    "nodemailer": "^6.7.2",
    "winston": "^3.8.1"
